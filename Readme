#docker
docker-compose build --no-cache
docker-compose up   
   
#Add-Migration
dotnet ef --startup-project services/MyWebPhoneStoreApi.Service migrations add InitialMigration --project src/MyWebPhoneStoreApi 

#Update-Migration
dotnet ef --startup-project services/MyWebPhoneStoreApi.Service database update InitialMigration --project src/MyWebPhoneStoreApi
